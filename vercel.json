{
  "builds": [
    {
      "src": "src/html/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/js/scripts.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/css/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/icons/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/png/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/html/index.html"
    },
    {
      "src": "/scripts.js",
      "dest": "/src/js/scripts.js"
    },
    {
      "src": "/styles.css",
      "dest": "/src/css/styles.css"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/public/icons/$1"
    },
    {
      "src": "/png/(.*)",
      "dest": "/public/png/$1"
    }
  ]
}
